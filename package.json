{
  "name": "eslint-sonarjs",
  "version": "0.0.0",
  "type": "module",
  "repository": "git+https://github.com/un-ts/eslint-plugin-sonar.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "SonarJS",
    "eslint-plugin-sonar"
  ],
  "packageManager": "yarn@4.7.0",
  "scripts": {
    "build": "run-s build:lib build:readme",
    "build:lib": "rollup -c",
    "build:readme": "node scripts/readme.js",
    "clean": "rimraf eslint-plugin-sonar/lib",
    "codesandbox:install": "git submodule update --init && yarn",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint . --cache --max-warnings 10 -f friendly",
    "lint:tsc": "tsc --noEmit",
    "prepare": "patch-package && simple-git-hooks || exit 0",
    "release": "node scripts/private.js && changeset publish",
    "test": "node eslint-plugin-sonar",
    "typecov": "type-coverage"
  },
  "devDependencies": {
    "@1stg/commitlint-config": "^4.0.0",
    "@1stg/lint-staged": "^4.0.0",
    "@1stg/remark-preset": "^2.0.0",
    "@1stg/simple-git-hooks": "^0.2.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^18.6.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-json": "^6.1.0",
    "@unts/patch-package": "^8.1.1",
    "eslint": "^8.57.0",
    "eslint-define-config": "^2.1.0",
    "eslint-formatter-friendly": "^7.0.0",
    "globals": "^15.2.0",
    "lint-staged": "^15.4.3",
    "minimatch": "^9.0.5",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "rollup": "^4.34.9",
    "rollup-plugin-ts": "^3.4.5",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^5.22.5",
    "type-coverage": "^2.27.1",
    "typescript": "5.4.3"
  },
  "resolutions": {
    "@types/estree": "1.0.5",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/experimental-utils": "^5.62.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@typescript-eslint/scope-manager": "^7.8.0",
    "@typescript-eslint/type-utils": "^7.8.0",
    "@typescript-eslint/utils": "^7.8.0",
    "browserslist-generator": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-sonar": "link:eslint-plugin-sonar",
    "eslint-plugin-sonarjs": "^1.0.3",
    "prettier": "^3.5.3",
    "tmp": "^0.2.1"
  },
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreCatch": true,
    "ignoreFiles": [
      "SonarJS/**/*"
    ],
    "ignoreNonNullAssertion": true,
    "strict": true,
    "update": true
  }
}
